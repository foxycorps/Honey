{
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build types",
    "types": "turbo run types",
    "dev:core": "turbo run dev --scope=\"@honeydocs/honey\"",
    "dev:theme-blog": "turbo run dev --scope=\"blog\" --include-dependencies",
    "dev:theme-docs": "turbo run dev --scope=\"docs\" --include-dependencies",
    "dev:minimal": "turbo run dev --scope=\"minimal\" --include-dependencies",
    "build:core": "pnpm run build --filter honey",
    "build:theme-blog": "pnpm run build --filter honey-theme-blog",
    "build:theme-docs": "pnpm run build --filter honey-theme-docs",
    "version-beta": "lerna version --preid beta",
    "publish-beta": "lerna publish from-package --canary --preid beta --pre-dist-tag beta",
    "test": "turbo run test"
  },
  "devDependencies": {
    "@tailwindcss/nesting": "^0.0.0-insiders.565cd3e",
    "@tailwindcss/typography": "^0.5.0",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "autoprefixer": "^10.4.1",
    "concurrently": "^7.0.0",
    "cssnano": "^4.1.11",
    "esbuild": "^0.14.10",
    "next": "^12.1.6",
    "postcss": "^8.4.5",
    "postcss-cli": "^8.3.1",
    "prettier": "^2.5.1",
    "prettier-plugin-tailwindcss": "^0.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "tailwindcss": "^3.0.23",
    "turbo": "^1.2.9",
    "typescript": "^4.6.2",
    "vitest": "^0.12.4"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none",
    "arrowParens": "avoid"
  },
  "packageManager": "pnpm@6.27.1"
}