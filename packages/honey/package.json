{
  "name": "@honeydocs/honey",
  "version": "1.0.0",
  "description": "Next.js and MDX based site generator.",
  "main": "index.js",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/*",
    "index.js",
    "ssg.js",
    "data.js",
    "loader.js",
    "locales.js",
    "context.js"
  ],
  "types": "./dist/types.d.ts",
